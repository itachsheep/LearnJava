<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Black.NoTitleBar.Fullscreen">
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@drawable/bg</item>
    </style>
    <style name="ms_submit">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#949494</item>
        <item name="android:background">@drawable/ms_submit_bg</item>
    </style>

    <!--

<item name="android:windowIsTranslucent">true</item>是否半透明
    -->
    <style name="dialog_custom">
        <item name="android:windowIsFloating">true</item>  <!--是否浮在界面上-->
         <!--是否半透明-->
        <item name="android:windowNoTitle">false</item>       <!--是否有标题-->
        <item name="android:windowBackground">@android:color/transparent</item> <!--窗口背景色透明-->
        <item name="android:backgroundDimEnabled">false</item> <!--背景是否模糊显示-->
    </style>
</resources>
